{
	"extends": "../tsconfig.json",
	"compilerOptions": {
		"lib": ["ESNext"],
		"target": "ESNext",
		"module": "ESNext",
		"jsx": "react-jsx",
		"jsxImportSource": "hono/jsx",
		"types": ["bun-types"],
		"moduleResolution": "bundler",
		"noEmit": true,
		"strict": true,
		"skipLibCheck": true
	},
	"include": ["src/**/*"],
	"exclude": ["node_modules"]
}
